-- PART 1/2: M1 Reset Tech v1.0 - Core functionality with Roblox notifications
-- All settings hardcoded - Copy this entire block and the next one into your executor

local vu1 = game:GetService("Players").LocalPlayer
local vu2 = game:GetService("UserInputService")
local vu3 = game:GetService("TweenService")
local vu4 = game:GetService("RunService")
local v5 = game:GetService("Workspace")
local vu7 = game:GetService("StarterGui")
game:GetService("ReplicatedStorage")
local vu6 = nil

pcall(function()
    vu6 = game:GetService("VirtualInputManager")
end)
if not vu6 then
    local v8 = rawget(_G, "VirtualInputManager")
    if v8 then
        vu6 = v8
    else
        local v9 = rawget(_G, "syn") and rawget(_G, "syn").virtual_input
        if v9 then
            vu6 = v9
        else
            vu6 = rawget(_G, "virtualinputmanager")
        end
    end
end

local vu10 = {
    M1_RESET = Enum.KeyCode.G  -- Changed from R to G as requested
}

getgenv().connections = getgenv().connections or {}
if type(getgenv().connections) == "table" then
    local v11, v12, v13 = ipairs(getgenv().connections)
    while true do
        local vu14
        v13, vu14 = v11(v12, v13)
        if v13 == nil then break end
        pcall(function()
            if vu14 and vu14.Disconnect then vu14:Disconnect() end
        end)
    end
end
getgenv().connections = {}

-- Roblox Native Notifications (using StarterGui:SetCore)
local function showRobloxNotification(title, text)
    pcall(function()
        vu7:SetCore("SendNotification", {
            Title = title;
            Text = text;
            Duration = 5;
            Button1 = "OK"
        })
    end)
end

-- Show notifications on script execution using Roblox system
task.spawn(function()
    wait(0.5)
    showRobloxNotification("M1 Reset Tech v1.0", "Join the discord for more updates & early access!\nhttps://discord.gg/PA2sUXezDD")
    wait(2)
    showRobloxNotification("Created by CPS Network", "Keybinds: G")
end)

local function vu21(p15)
    local v16 = p15 or 2
    if type(gethui) == "function" then
        local v17, v18 = pcall(gethui)
        if v17 and v18 then return v18 end
    end
    if vu1 then
        local v19 = tick()
        while tick() - v19 < v16 do
            if vu1:FindFirstChild("PlayerGui") then return vu1.PlayerGui end
            task.wait(0.05)
        end
        if vu1:FindFirstChild("PlayerGui") then return vu1.PlayerGui end
    end
    local v20, v21 = pcall(function() return game:GetService("CoreGui") end)
    return v20 and v21 and v21 or game:GetService("StarterGui")
end
-- END OF PART 1/2 - Continue to PART 2/2 for complete script [file:1]
-- PART 2/2: M1 Reset Tech v1.0 - Smaller draggable Golden-Black button
-- Paste this immediately after PART 1/2 - All requested changes implemented

local vu22 = vu21(3)
pcall(function()
    local v23, v24, v25 = ipairs({"M1_RESET_UI", "TSB_UI", "M1ResetUI"})
    while true do
        local v26
        v25, v26 = v23(v24, v25)
        if v25 == nil then break end
        local v27 = vu22:FindFirstChild(v26)
        if v27 then v27:Destroy() end
    end
end)

local function v30(pu28)
    local vu29 = rawget(_G, "syn")
    if vu29 and type(vu29.protect_gui) == "function" then
        pcall(function() vu29.protect_gui(pu28) end)
    end
end

local vu47 = v5.CurrentCamera
local vu45 = false
local vu46 = {}

local function vu50()
    local v48 = vu1.Character
    local v49 = v48 and v48:FindFirstChildOfClass("Humanoid")
    return v48, v49
end

local function vu59()
    local _, v51 = vu50()
    local vu52 = v51 and v51:FindFirstChildOfClass("Animator")
    if vu52 then
        local v53, v54 = pcall(function() return vu52:GetPlayingAnimationTracks() end)
        if v53 and v54 then
            local v55, v56, v57 = ipairs(v54)
            while true do
                local vu58
                v57, vu58 = v55(v56, v57)
                if v57 == nil then break end
                pcall(function() vu58:Stop() end)
            end
        end
    end
end

local function vu66(p60)
    local _, v61 = vu50()
    if v61 then
        local vu62 = v61:FindFirstChildOfClass("Animator")
        if not vu62 then
            vu62 = Instance.new("Animator")
            vu62.Parent = v61
        end
        local vu63 = vu46[p60]
        if not vu63 then
            vu63 = Instance.new("Animation")
            vu63.AnimationId = "rbxassetid://" .. tostring(p60)
            vu46[p60] = vu63
        end
        local v64, vu65 = pcall(function()
            return vu62:LoadAnimation(vu63)
        end)
        if v64 and vu65 then
            vu65.Priority = Enum.AnimationPriority.Action
            vu65:Play()
            pcall(function() vu65:AdjustSpeed(1.2) end)
            return vu65
        end
    end
end

local function vu75(pu67)
    if not vu45 then
        vu45 = true
        local vu68, _ = vu50()
        if vu68 then vu68 = vu68:FindFirstChild("HumanoidRootPart") end
        if vu68 then
            pcall(function()
                vu68.CFrame = vu68.CFrame * CFrame.Angles(0, math.rad(pu67), 0)
                local v69 = vu47 and vu47.CFrame.Position or vu68.Position + Vector3.new(0, 5, 0)
                local v70 = v69.Y
                local v71 = v69 - vu68.Position
                local v72 = Vector3.new(v71.X, 0, v71.Z)
                local v73 = CFrame.fromAxisAngle(Vector3.yAxis, math.rad(pu67)) * v72
                local v74 = vu68.Position + v73
                if vu47 and vu47:IsA("Camera") then
                    vu47.CFrame = CFrame.lookAt(Vector3.new(v74.X, v70, v74.Z), vu68.Position)
                end
            end)
        end
        vu45 = false
    end
end

local function vu81(p76, p77, p78)
    if not vu45 then
        vu45 = true
        local v79, _ = vu50()
        if v79 then v79 = v79:FindFirstChild("HumanoidRootPart") end
        if v79 then
            local vu80 = vu3:Create(v79, TweenInfo.new(p78, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
                CFrame = v79.CFrame * CFrame.new(p76 * p77, 0, 0)
            })
            pcall(function() vu80:Play() end)
            if vu80 then pcall(function() vu80.Completed:Wait() end) end
            pcall(function() vu80:Destroy() end)
        end
        vu45 = false
    end
end

-- M1 Reset Function (Hardcoded settings - triggers on button press/keybind G)
local function vu91()
    vu59()
    pcall(function() vu66(10480793962) end)
    pcall(function() vu81(1, 26, 0.22) end)
    pcall(function() vu75(70) end)
    task.wait(0.003)
    pcall(function()
        if vu6 and type(vu6.SendKeyEvent) == "function" then
            vu6:SendKeyEvent(true, Enum.KeyCode.Q, false, game)
            vu6:SendKeyEvent(false, Enum.KeyCode.Q, false, game)
        end
    end)
end

-- NEW GUI: Smaller Golden-Black gradient button (draggable, fade-in only)
local vu93 = Instance.new("ScreenGui")
vu93.Name = "M1ResetCPS"
vu93.ResetOnSpawn = false
vu93.IgnoreGuiInset = true
vu93.DisplayOrder = 9999
vu93.ZIndexBehavior = Enum.ZIndexBehavior.Global
vu93.Parent = vu21(2)
v30(vu93)

-- Smaller main button container (16:9 aspect ratio - much smaller)
local mainBtn = Instance.new("Frame", vu93)
mainBtn.Name = "M1ResetButton"
mainBtn.Size = UDim2.new(0, 144, 0, 81)  -- Smaller 16:9 (144x81)
mainBtn.Position = UDim2.new(0.5, -72, 0.5, -40.5)
mainBtn.BackgroundColor3 = Color3.fromRGB(212, 175, 55)  -- Golden base
mainBtn.BorderSizePixel = 0
mainBtn.BackgroundTransparency = 1  -- Start invisible for fade-in

-- Golden-Black gradient
local gradient = Instance.new("UIGradient", mainBtn)
gradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 215, 0)),    -- Top: Gold
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 0))         -- Bottom: Black
}
gradient.Rotation = 45

-- Curved borders for main button (no white stroke)
local mainCorner = Instance.new("UICorner", mainBtn)
mainCorner.CornerRadius = UDim.new(0, 12)

-- Smaller black button overlay
local topBtn = Instance.new("TextButton", mainBtn)
topBtn.Size = UDim2.new(1, -8, 1, -8)  -- Slightly smaller
topBtn.Position = UDim2.new(0, 4, 0, 4)
topBtn.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
topBtn.Text = "M1 Reset"
topBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
topBtn.Font = Enum.Font.GothamBold
topBtn.TextSize = 16
topBtn.BorderSizePixel = 0
topBtn.BackgroundTransparency = 1  -- Start invisible for fade-in

-- Curved borders for top button
local topCorner = Instance.new("UICorner", topBtn)
topCorner.CornerRadius = UDim.new(0, 10)

-- FADE-IN animation only (no other animations)
task.wait(1)
vu3:Create(mainBtn, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
    BackgroundTransparency = 0
}):Play()
vu3:Create(topBtn, TweenInfo.new(0.8, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
    BackgroundTransparency = 0
}):Play()

-- Button click triggers M1 Reset
topBtn.MouseButton1Click:Connect(function()
    task.spawn(vu91)
end)

-- Draggable functionality
local dragging = false
local dragStart = nil
local startPos = nil

mainBtn.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainBtn.Position
    end
end)

mainBtn.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        mainBtn.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

vu2.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

-- Keybind G support
local keybindConn = vu2.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if vu2:GetFocusedTextBox() then return end
    if input.KeyCode == vu10.M1_RESET then
        vu91()
    end
end)

table.insert(getgenv().connections, keybindConn)

print("M1 Reset Tech v1.0 loaded! Press G or drag the small golden button.[file:1]")
-- END OF PART 2/2 - Full script complete! Roblox notifications, smaller draggable button, golden gradient, fade-in only [file:1]
